{
  "name": "multiple-paradigms",
  "private": false,
  "version": "0.0.0",
  "description": "Implement a Markdown parser using three different programming paradigms: **object-oriented**, **functional**, and **imperative**.",
  "devDependencies": {
    "chai": "~1.8.0",
    "mocha": "2.0.1"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register ./spec/*.js",
    "parse:oo": "babel-node bin/parse --paradigm oo",
    "parse:functional": "babel-node bin/parse --paradigm functional",
    "parse:imperative": "babel-node bin/parse --paradigm imperative"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "minimist": "^1.2.0"
  }
}
